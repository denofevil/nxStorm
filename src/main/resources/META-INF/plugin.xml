<idea-plugin url="https://nrwl.io/nx/plugins">
    <name>nxStorm</name>
    <description>Support for Nrwl Extensions for Angular</description>
    <vendor>Nrwl</vendor>

    <depends>JavaScript</depends>
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <directoryProjectGenerator implementation="io.nrwl.project.NxProjectGenerator"/>
        <projectTemplatesFactory implementation="io.nrwl.project.NxTemplateFactory"/>
    </extensions>
</idea-plugin>
